{
  "name": "cuteanime.moe",
  "version": "1.0.0",
  "description": "Cuteanime is a website where you can watch anime with Japanese subtitles, it is a fun way to study japanese.",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run clean && webpack && node --experimental-wasm-threads dist/server.js",
    "build": "npm run clean && webpack && npm run redirects",
    "redirects": "echo \"/* /index.html 200\" | cat > dist/_redirects",
    "server": "webpack && npm run pm2",
    "pm2": "pm2 stop all && pm2 start dist/server.js",
    "clean": "del-cli ./dist"
  },
  "keywords": [
    "cute",
    "anime",
    "japanese",
    "subtitles",
    "moe",
    "kawaii",
    "learn japanese",
    "japanese learning",
    "cuteanime",
    "cutemanga"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tenpi/Cuteanime.moe"
  },
  "author": "Moepi",
  "license": "MIT",
